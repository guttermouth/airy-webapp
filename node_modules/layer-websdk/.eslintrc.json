{
  "extends": "airbnb",
  "rules": {
    "no-else-return": 0,
    "default-case": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "one-var": [2, {"var": "always", "let": "always", "const": "never"}],
    "no-param-reassign": 0,
    "brace-style": 0,
    "max-len": [2, {"code": 120, "tabWidth": 2, "ignoreUrls": true, "ignoreComments": true, "ignoreTemplateLiterals": true}],
    "consistent-return": 0,
    "prefer-template": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"args": "none"}],
    "operator-assignment": 0,
    "no-restricted-syntax": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "no-prototype-builtins": 0,
    "comma-dangle": [2, {"functions": "never", "arrays": "always-multiline", "objects": "always-multiline"}],
    "no-restricted-properties": 0
  },
  "env": {
      "browser": true,
      "node": true,
      "jasmine": true
  }
}