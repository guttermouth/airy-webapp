{
  "extends": "airbnb",
  "env": {
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "max-len": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    }
  },
  "plugins": [
    "react"
  ]
}
